{
  "name": "lively.ast",
  "version": "0.9.15",
  "description": "Parsing JS code into ASTs and tools to query and transform these trees.",
  "main": "dist/lively.ast.js",
  "scripts": {
    "test": "mocha-es6 tests/*-test.js",
    "build": "node ./tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LivelyKernel/lively.ast.git"
  },
  "keywords": [
    "LivelyWeb",
    "parser",
    "parsing",
    "estree",
    "ast",
    "lively"
  ],
  "author": "Robert Krahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LivelyKernel/lively.ast/issues"
  },
  "homepage": "https://github.com/LivelyKernel/lively.ast",
  "dependencies": {
    "lively.lang": "^1.0.0"
  },
  "devDependencies": {
    "acorn": "^4.0.3",
    "acorn-es7-plugin": "^1.1.3",
    "acorn-object-spread": "LivelyKernel/acorn-object-spread#0da456",
    "babel-core": "^6.16.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.16.0",
    "estree-to-js": "^0.2.0",
    "mocha-es6": "^0.5",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1"
  },
  "systemjs": {
    "main": "index.js",
    "map": {
      "lively.ast": ".",
      "escodegen": "./dist/escodegen.browser.js",
      "acorn": "./dist/acorn.js",
      "util": {
        "node": "@node/util",
        "~node": "@empty"
      },
      "child_process": {
        "node": "@node/child_process",
        "~node": "@empty"
      }
    },
    "meta": {
      "dist/escodegen.browser.js": {
        "format": "global"
      },
      "dist/acorn.js": {
        "format": "global",
        "exports": "acorn"
      }
    }
  }
}
