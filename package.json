{
  "name": "lively.ast",
  "version": "0.6.11",
  "description": "Parsing JS code into ASTs and tools to query and transform these trees.",
  "main": "dist/lively.ast.js",
  "scripts": {
    "test": "mocha-es6 tests/*-test.js",
    "build": "node ./tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LivelyKernel/lively.ast.git"
  },
  "keywords": [
    "LivelyWeb",
    "parser",
    "parsing",
    "estree",
    "ast",
    "lively"
  ],
  "author": "Robert Krahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LivelyKernel/lively.ast/issues"
  },
  "homepage": "https://github.com/LivelyKernel/lively.ast",
  "dependencies": {
    "acorn-es7-plugin": "^1.0.13",
    "lively.lang": "^0.5.17"
  },
  "devDependencies": {
    "acorn": "^2.7.0",
    "babel-core": "^5.8.35",
    "babel-preset-es2015-rollup": "^1.1.1",
    "estree-to-js": "^0.1.4",
    "mocha-es6": "^0.2.7",
    "mocha-phantomjs-core": "^1.3.0",
    "rollup": "^0.26.1",
    "rollup-plugin-babel": "^2.4.0"
  },
  "systemjs": {
    "main": "index.js",
    "map": {
      "lively.ast": ".",
      "escodegen": "./dist/escodegen.browser.js",
      "acorn": "./dist/acorn.js",
      "util": {
        "node": "@node/util",
        "~node": "@empty"
      },
      "child_process": {
        "node": "@node/child_process",
        "~node": "@empty"
      }
    },
    "meta": {
      "dist/escodegen.browser.js": {
        "format": "global"
      },
      "dist/acorn.js": {
        "format": "global",
        "exports": "acorn"
      }
    }
  },
  "lively": {
    "main": "index.js",
    "packageMap": {
      "lively.lang": "./node_modules/lively.lang"
    }
  }
}
